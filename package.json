{
  "name": "todo-api",
  "version": "1.0.0",
  "main": "server.js",
  "description": "Scalable Todo API with user authentication, built with Node.js, Express, and MongoDB",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t todo-api .",
    "docker:run": "docker-compose up --build",
    "docker:scale": "docker-compose up --scale todo-app=3"
  },
  "keywords": ["todo", "api", "nodejs", "express", "mongodb", "jwt", "redis", "docker"],
  "author": "Krish",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "redis": "^4.6.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
